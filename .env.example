# OpenRouter API settings (Required)
API_KEY=your_openrouter_api_key_here
API_BASE=https://openrouter.ai/api/v1
MODEL=anthropic/claude-sonnet-4

# GitHub OAuth settings (optional)
GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=
JWT_SECRET=your_jwt_secret_here

# Google Ads settings (optional)
AD_CLIENT=
AD_SLOT=

# Cache settings
CACHE_CLIENT_TYPE=memory
# For Redis cache (optional)
KV_URL=
# For Upstash cache (optional)
KV_REST_API_URL=
KV_REST_API_TOKEN=

# Rate limiting
ENABLE_RATE_LIMIT=true